emo(gr2.Top,iLeft2,mWidth2,&#13;&#10;                       gr2.Height,&#13;&#10;                       s2,&#13;&#10;                       gr2.Font.Name,&#13;&#10;                       gr2.Font.Size,&#13;&#10;                       gr2.Rotation);&#13;&#10;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure create_memo(xTop,xLeft,xWidth,xHeight: Extended;&#13;&#10;       xText,xFont_Name: String; xFont_Size, xRotation: Integer);&#13;&#10;var&#13;&#10;  Memo: TfrxMemoView;&#13;&#10;begin&#13;&#10;  Memo := TfrxMemoView.Create(ReportTitle1);&#13;&#10;  Memo.Top := xTop;&#13;&#10;  Memo.Left := xLeft;&#13;&#10;  Memo.Width := xWidth;&#13;&#10;  Memo.Height := xHeight;&#13;&#10;  Memo.Text := xText;&#13;&#10;  Memo.Frame.Typ := ftLeft  +ftRight + ftTop + ftBottom;&#13;&#10;  Memo.VAlign := vaCenter;&#13;&#10;  Memo.HAlign := haCenter;&#13;&#10;  Memo.Font.Name := xFont_Name;&#13;&#10;  Memo.Font.Size := xFont_Size;&#13;&#10;  Memo.Rotation := xRotation;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure DBCross1OnCalcWidth(ColumnIndex: Integer; ColumnValues: Variant; var Width: Extended);&#13;&#10;begin&#13;&#10;  Width := Rik.Width;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure DBCross1OnPrintCell(Memo: TfrxMemoView; RowIndex, ColumnIndex, CellIndex: Integer; RowValues, ColumnValues, Value: Variant);&#13;&#10;var s: String;&#13;&#10;    x: Integer;&#13;&#10;    v: Double;&#13;&#10;begin&#13;&#10;  if Value = 0 then&#13;&#10;    Memo.Text := '';&#13;&#10;&#13;&#10;  if ((Pos('Напрям ',RowValues[0]) &#62; 0)&#13;&#10;       or (Pos('Разом:',RowValues[0]) &#62; 0)) then&#13;&#10;  begin&#13;&#10;    Memo.Font.Style := fsBold + fsUnderline;&#13;&#10;  end;&#13;&#10;&#13;&#10;  x := DBCross1.ColCount - ColumnIndex;&#13;&#10;  if ((x &#60; 5) and (Value &#60;&#62; 0)) then Memo.Text := FormatFloat('##0.00',Value/1000);&#13;&#10;  if ((is_proc[ColumnIndex] &#60;&#62; 0) and (Value &#60;&#62; 0)) then Memo.Text := FormatFloat('##0.0',Value/10);&#13;&#10;{  if ((DBCross1.ColCount &#62;= 28) and ((x &#60; 13) or (x = 15)))&#13;&#10;      or ((DBCross1.ColCount &#60; 27) and ((x &#60; 8) or (x = 10))) then&#13;&#10;    if (Value &#60;&#62; 0) then&#13;&#10;      if ((DBCross1.ColCount &#62;= 28) and (x &#60; 5))&#13;&#10;         or ((DBCross1.ColCount &#60; 27) and (x &#60; 3)) then&#13;&#10;           Memo.Text := FormatFloat('##0.00',Value/1000)&#13;&#10;         else&#13;&#10;           Memo.Text := FormatFloat('##0.0',Value/10);}&#13;&#10;end;&#13;&#10;&#13;&#10;procedure DBCross1OnPrintRowHeader(Memo: TfrxMemoView; HeaderIndexes, HeaderValues, Value: Variant);&#13;&#10;var s: String;&#13;&#10;    k: Integer;&#13;&#10;begin&#13;&#10;  if ((Pos('Напрям ',Value) &#62; 0)&#13;&#10;       or (Pos('Разом:',Value) &#62; 0)) then&#13;&#10;  begin&#13;&#10;    Memo.Font.Style := fsUnderline;&#13;&#10;  end;&#13;&#10;&#13;&#10;end;&#13;&#10;&#13;&#10;begin&#13;&#10;&#13;&#10;end." OnStartReport="frxReport1OnStartReport" PropData="">
