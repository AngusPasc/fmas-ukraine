.Width;&#13;&#10;&#13;&#10;    gri1 := &#60;FDBDataset1.&#34;NPP_GR1&#34;&#62;;&#13;&#10;    gri2 := &#60;FDBDataset1.&#34;NPP_GR2&#34;&#62;;&#13;&#10;&#13;&#10;    if (old_gr1 = 1) then&#13;&#10;      s1 := 'Ліцензований обсяг прийому'&#13;&#10;    else if (old_gr1 = 2) then&#13;&#10;      s1 := 'Обсяг прийому за державним (галузевим) замовленням'&#13;&#10;    else if (old_gr1 = 3) then&#13;&#10;        s1 := 'Подано заяв'&#13;&#10;    else if (old_gr1 = 4) then&#13;&#10;        s1 := 'Зараховано на навчання'&#13;&#10;    else if (old_gr1 = 5) then&#13;&#10;      s1 := 'Виконання ліцензованого обсягу прийому, %'&#13;&#10;    else if (old_gr1 = 6) then&#13;&#10;      s1 := 'Виконання державного (галузевого) замовлення, %'&#13;&#10;    else if (old_gr1 = 7) then&#13;&#10;      s1 := 'Частка студентів, які зараховані за державним (галузевим) замовленням'&#13;&#10;    else if (old_gr1 = 8) then&#13;&#10;        s1 := 'Конкурс на одне місце';&#13;&#10;&#13;&#10;    if (old_gr1 = 3) then&#13;&#10;        if (old_gr2 = 1) then&#13;&#10;          s2 := 'Всього'&#13;&#10;        else&#13;&#10;          s2 := 'У т.ч. за державним (галузевим) замовленням';&#13;&#10;&#13;&#10;    if (old_gr1 = 4) then&#13;&#10;        if (old_gr2 = 1) then&#13;&#10;          s2 := 'Всього'&#13;&#10;        else&#13;&#10;          s2 := 'У т.ч. за державним (галузевим) замовленням';&#13;&#10;&#13;&#10;    if (old_gr1 = 8) then&#13;&#10;        if (old_gr2 = 1) then&#13;&#10;          s2 := 'Всього'&#13;&#10;        else&#13;&#10;          s2 := 'У т.ч. за державним (галузевим) замовленням';&#13;&#10;&#13;&#10;    if (old_gr1 = 3) or (old_gr1 = 4) or (old_gr1 = 8) then&#13;&#10;    begin&#13;&#10;      if (old_gr1 &#60;&#62; gri1) then&#13;&#10;      begin&#13;&#10;       create_memo(gr1.Top,iLeft1,mWidth1,&#13;&#10;                       gr1.Height,&#13;&#10;                       s1,&#13;&#10;                       gr1.Font.Name,&#13;&#10;                       gr1.Font.Size,&#13;&#10;        