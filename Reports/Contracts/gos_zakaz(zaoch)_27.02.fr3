    <TfrxMemoView Name="Memo105" Left="3,77953" Top="139,84261" Width="480,00031" Height="34,01577" Font.Charset="1" Font.Color="0" Font.Height="-12" Font.Name="Times New Roman" Font.Style="0" Memo.Text=" 3.4. Надати можливість працевлаштування Студенту - випускнику за отриманим освітньо-кваліфікаційним рівнем і фахом." ParentFont="False"/>
    <TfrxMemoView Name="Memo106" ShiftMode="smDontShift" Left="3,77953" Top="224,88187268" Width="34,01577" Height="18,89765" Font.Charset="1" Font.Color="0" Font.Height="-12" Font.Name="Times New Roman" Font.Style="0" Memo.Text=" 4.2." ParentFont="False"/>
    <TfrxMemoView Name="Memo107" Left="0" Top="324,28345724" Width="34,01577" Height="18,89765" Font.Charset="1" Font.Color="0" Font.Height="-12" Font.Name="Times New Roman" Font.Style="0" Memo.Text=" 4.6." ParentFont="False"/>
