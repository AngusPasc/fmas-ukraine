<TfrxReport Name="" Version="3.15" DotMatrixReport="False" IniFile="\Software\Fast Reports" PreviewOptions.Buttons="2047" PreviewOptions.Zoom="1" PrintOptions.Copies="2" PrintOptions.Printer="Default" PrintOptions.Reverse="True" ReportOptions.CreateDate="39975,4288395139" ReportOptions.Description.Text="" ReportOptions.LastChange="42652,9191932176" ScriptLanguage="PascalScript" ScriptText.Text="//var d1,m1,y1,d2,m2,y2:Word;&#13;&#10;var&#13;&#10; kol_month : Word;&#13;&#10;&#13;&#10;procedure Memo190OnBeforePrint(Sender: TfrxComponent);&#13;&#10;var&#13;&#10;  d, m, y : Double;&#13;&#10;begin&#13;&#10;  memo190.text := '';&#13;&#10;&#13;&#10;  y := kol_month / 10;&#13;&#10;  m := kol_month - (y * 10);&#13;&#10;&#13;&#10;  if (y = 0)  and (m &#60;&#62; 0) then memo190.text := '1';&#13;&#10;  if (y &#60;&#62; 0) and (m &#60;&#62; 0) then memo190.text := FloatToStr(y) + ',' + FloatToStr(m);&#13;&#10;  if (y &#60;&#62; 0) and (m = 0)  then memo190.text := FloatToStr(y);&#13;&#10;end;&#13;&#10;&#13;&#10;procedure Page1OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  if &#60;Draft&#62; = 1&#13;&#10;   then&#13;&#10;    Begin&#13;&#10;     Memo106.visible := true;&#13;&#10;    // Memo122.visible := true;&#13;&#10;    End&#13;&#10;   else&#13;&#10;    Begin&#13;&#10;     Memo106.visible := false;&#13;&#10;    // Memo122.visible := false;&#13;&#10;    End;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure BarCode1OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  if &#60;PD_DBDataset.&#34;BARCODE&#34;&#62; &#62;0&#13;&#10;   then BarCode1.text := &#60;PD_DBDataset.&#34;BARCODE&#34;&#62;&#13;&#10;   else BarCode1.visible := false;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure Memo117OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;end;&#13;&#10;&#13;&#10;procedure Memo56OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;end;&#13;&#10;&#13;&#10;procedure Memo53OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;end;&#13;&#10;&#13;&#10;procedure Memo54OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;end;&#13;&#10;&#13;&#10;procedure Memo57OnBeforePrint(Sender: TfrxComponent);&#13;&#10;var&#13;&#10;  db, mb, yb, de, me, ye : Word;&#13;&#10;  S : integer;&#13;&#10;begin&#13;&#10;  DecodeDate(&#60;PD_DBDataset_stage_opl.&#34;DATE_BEG3_FIZ&#34;&#62;, yb, mb, db);&#13;&#10;  DecodeDate(&#60;PD_DBDataset_stage_opl.&#34;DATE_END3_FIZ&#34;&#62;, ye, me, de);&#13;&#10;&#13;&#10;  if &#60;PD_DBDataset_stage_opl.&#34;DATE_BEG3_FIZ&#34;&#62; = 0 then&#13;&#10;   Begin&#13;&#10;    DecodeDate(&#60;PD_DBDataset_stage_opl.&#34;DATE_BEG3_JUR&#34;&#62;, yb, mb, db);&#13;&#10;    DecodeDate(&#60;PD_DBDataset_stage_opl.&#34;DATE_END3_JUR&#34;&#62;, ye, me, de);&#13;&#10;   End;&#13;&#10;&#13;&#10;  S := me - mb;&#13;&#10;&#13;&#10;  if (ye - yb) &#60;&#62; 0 then S := S + 2;&#13;&#10;&#13;&#10;  if (de-db) &#62; 0 then S := S + 1;&#13;&#10;&#13;&#10;  S := S + ((ye - yb) * 10);&#13;&#10;&#13;&#10;  kol_month := kol_month + S;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure Memo45OnBeforePrint(Sender: TfrxComponent);&#13;&#10;var&#13;&#10;  db, mb, yb, de, me, ye : Word;&#13;&#10;  S : integer;&#13;&#10;begin&#13;&#10;  DecodeDate(&#60;PD_DBDataset_stage_opl.&#34;DATE_BEG1_FIZ&#34;&#62;, yb, mb, db);&#13;&#10;  DecodeDate(&#60;PD_DBDataset_stage_opl.&#34;DATE_END1_FIZ&#34;&#62;, ye, me, de);&#13;&#10;&#13;&#10;  if &#60;PD_DBDataset_stage_opl.&#34;DATE_BEG1_FIZ&#34;&#62; = 0 then&#13;&#10;   Begin&#13;&#10;    DecodeDate(&#60;PD_DBDataset_stage_opl.&#34;DATE_BEG1_JUR&#34;&#62;, yb, mb, db);&#13;&#10;    DecodeDate(&#60;PD_DBDataset_stage_opl.&#34;DATE_END1_JUR&#34;&#62;, ye, me, de);&#13;&#10;   End;&#13;&#10;&#13;&#10;  S := me - mb;&#13;&#10;&#13;&#10;  if (ye - yb) &#60;&#62; 0 then S := S + 2;&#13;&#10;&#13;&#10;  if (de-db) &#62; 0 then S := S + 1;&#13;&#10;&#13;&#10;  S := S + ((ye - yb) * 10);&#13;&#10;&#13;&#10;  kol_month := kol_month + S;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure Memo51On