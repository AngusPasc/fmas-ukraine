    <TfrxMemoView Name="Memo117" Left="294,80334" Top="634,96104" Width="419,52783" Height="34,01577" OnBeforePrint="Memo117OnBeforePrint" DataSet="PD_DBDataset" DataSetName="PD_DBDataset" Font.Charset="1" Font.Color="0" Font.Height="-13" Font.Name="Times New Roman" Font.Style="0" Memo.Text="" ParentFont="False"/>
